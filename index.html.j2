{% block firstpagecontent %}
<h4 style="text-align:center">Les dernières épreuves</h4>

<div class="table-responsive">
	<table class="table table-striped table-hover">
		<tbody><tr>
			<th>Date</th>
			<th>Nom</th>
			<th>Discipline</th>
			<th>Organisateur</th>
      <th class="text-center">Classements</th>
		</tr>

    {% for x, line in last_calendar.iterrows() %}
    <tr class="">
      <td class="">{{line["Date"]}}</td>
      <td class="">{{line["Nom"]}}
				{%- if line["Info"]|length >0 -%}
					<br><font color="red"><u>{{line["Info"]}}</u></font>
				{% endif %}
			</td>
			<td class="">{{line["Discipline"]}}</td>
      <td class="">{{line["Club"]}}</td>
      <td class="text-center">
        {% if line["resul_dispo"] == True %}
        <a href="{{ line["lien_resul"] }}">
        <img  src="./Styling/images/podium.png" width="26" height="22"></a>
        {% endif %}
				{% if line["resul1_dispo"] == True %}
				<a href="{{ line["lien_resul1"] }}">
				<img  src="./Styling/images/podium.png" width="26" height="22"></a>
				{% endif %}

      </td>
    </tr>
    {% endfor %}

	</tbody></table>
</div>

<h4 style="text-align:center">Les prochaines épreuves</h4>
<div class="table-responsive">
